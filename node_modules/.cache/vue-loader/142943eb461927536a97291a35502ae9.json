\n        <span class=\"visually-hidden\">edit for {{label}}</span>\r\n      </button>\r\n    </div>\r\n  </form>\r\n</template>\r\n<script>\r\nexport default {\r\n  props: {\r\n    label: {\r\n      type: String,\r\n      required: true\r\n    },\r\n    id: {\r\n      type: String,\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      newLabel: this.label\r\n    };\r\n  },\r\n  methods: {\r\n    onSubmit() {\r\n      if (this.newLabel && this.newLabel !== this.label) {\r\n        this.$emit(\"item-edited\", this.newLabel);\r\n      }\r\n    },\r\n    onCancel() {\r\n      this.$emit(\"edit-cancelled\");\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\n.edit-label {\r\n  font-family: Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  color: #0b0c0c;\r\n  display: block;\r\n  margin-bottom: 5px;\r\n}\r\ninput {\r\n  display: inline-block;\r\n  margin-top: 0.4rem;\r\n  width: 100%;\r\n  min-height: 4.4rem;\r\n  padding: 0.4rem 0.8rem;\r\n  border: 2px solid #565656;\r\n}\r\nform {\r\n  display: flex;\r\n  flex-direction: row;\r\n  flex-wrap: wrap;\r\n}\r\nform > * {\r\n  flex: 0 0 100%;\r\n}\r\n</style>"]}]}